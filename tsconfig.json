{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "allowJs": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "removeComments": true,
    "importHelpers": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "lib": ["esnext", "dom"],
    "types": [
      "vite/client",
      "@intlify/vite-plugin-vue-i18n/client",
      // "typed-router.d.ts"
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "env.d.ts",
    "types/**/*.d.ts",
    "auto-imports.d.ts",
    "typed-router.d.ts",
    "components.d.ts",
    "components-global.d.ts",
    "src/**/*.js",
    "src/**/*.ts",
    "src/**/*.vue"
, "src/components/forms/Diplom/.vue.ts"  ],
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
